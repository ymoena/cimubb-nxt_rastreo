<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".NXT">

    <RelativeLayout
        android:id="@+id/sensorLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <Button
            android:id="@+id/configure"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="65dp"
            android:gravity="center"

            android:text="Configurar" />


        <TextView
            android:id="@+id/sensor1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView10"
            android:layout_alignEnd="@+id/tituloSensores"
            android:layout_alignRight="@+id/tituloSensores"
            android:layout_alignTop="@+id/textView10"
            android:layout_marginLeft="30dp"
            android:layout_marginStart="30dp"
            android:layout_toRightOf="@+id/textView11"
            android:text="" />

        <TextView
            android:id="@+id/sensor2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView11"
            android:layout_alignEnd="@+id/sensor1"
            android:layout_alignLeft="@+id/sensor1"
            android:layout_alignRight="@+id/sensor1"
            android:layout_alignStart="@+id/sensor1"
            android:layout_alignTop="@+id/textView11"
            android:text="" />

        <TextView
            android:id="@+id/sensor3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView13"
            android:layout_alignEnd="@+id/sensor2"
            android:layout_alignLeft="@+id/sensor2"
            android:layout_alignRight="@+id/sensor2"
            android:layout_alignStart="@+id/sensor2"
            android:layout_alignTop="@+id/textView12"
            android:text="" />

        <TextView
            android:id="@+id/tituloSensores"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="110dp"
            android:gravity="center_horizontal"
            android:text="Control de Sensores"
            android:textSize="35sp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/tituloSensores"
            android:layout_alignStart="@+id/tituloSensores"
            android:layout_below="@+id/tituloSensores"
            android:layout_marginTop="45dp"
            android:gravity="center_horizontal"
            android:text="Ultrasonido :"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView10"
            android:layout_alignStart="@+id/textView10"
            android:layout_below="@+id/textView10"
            android:layout_marginTop="36dp"
            android:gravity="center_horizontal"
            android:text="Sensor de Luz 1:"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView11"
            android:layout_alignStart="@+id/textView11"
            android:layout_below="@+id/textView11"
            android:layout_marginTop="36dp"
            android:gravity="center_horizontal"
            android:text="Sensor de Luz 2:"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView12"
            android:layout_alignStart="@+id/textView12"
            android:layout_below="@+id/textView12"
            android:layout_marginTop="36dp"
            android:gravity="center_horizontal"
            android:text="Sensor de Luz 3:"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/sensor4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView13"
            android:layout_alignEnd="@+id/sensor3"
            android:layout_alignLeft="@+id/sensor3"
            android:layout_alignRight="@+id/sensor3"
            android:layout_alignStart="@+id/sensor3"
            android:layout_alignTop="@+id/textView13"
            android:text="" />

        <TextView
            android:id="@+id/instrucciones"
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:layout_below="@+id/textView13"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="36dp"
            android:text="Inicie el programa CIM-NXT en el robot para recibir los datos" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/configureLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        >

        <LinearLayout

            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView4"
            android:layout_marginTop="51dp">


            <TextView
                android:id="@+id/text_linea"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />

            <Button
                android:id="@+id/capturar_linea"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:text="Capturar Linea" />

        </LinearLayout>

        <LinearLayout

            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/line_follower"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="52dp">


            <TextView
                android:id="@+id/text_fondo"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />

            <Button
                android:id="@+id/capturar_fondo"
                android:layout_width="wrap_content"

                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:text="Capturar Fondo" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="40dp"
            android:text="Selecciona los colores"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <Button
            android:id="@+id/line_follower"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="62dp"
            android:text="Line Follower" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/ejecucionLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/prueba"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/reaccion_20cm"
            android:layout_alignStart="@+id/reaccion_20cm"
            android:layout_below="@+id/reaccion_20cm"
            android:text="prueba" />

        <Button
            android:id="@+id/tiempo_adelante"
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="80dp"
            android:text="Por tiempo\n(Adelante)" />

        <Button
            android:id="@+id/rotacion_adelante"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="80dp"
            android:layout_toEndOf="@+id/tiempo_adelante"
            android:layout_toRightOf="@+id/tiempo_adelante"
            android:text="Rotaciones\n(Adelante)" />

        <Button
            android:id="@+id/tiempo_atras"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="160dp"
            android:text="Tiempo\n(atras)" />

        <Button
            android:id="@+id/rotacion_atras"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="160dp"
            android:layout_toRightOf="@+id/tiempo_atras"
            android:text="Rotacion\n(atras)" />

        <Button
            android:id="@+id/reaccion_10cm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginLeft="10dp"

            android:layout_marginTop="230dp"

            android:text="Reaccion\n(10 cm)" />

        <Button
            android:id="@+id/reaccion_15cm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginLeft="10dp"
            android:layout_marginTop="230dp"
            android:layout_toRightOf="@+id/reaccion_10cm"

            android:text="Reaccion\n(15 cm)" />

        <Button
            android:id="@+id/reaccion_20cm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginLeft="15dp"
            android:layout_marginTop="300dp"
            android:text="Reaccion\n(20cm)" />

        <Button
            android:id="@+id/reaccion_25cm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginLeft="10dp"

            android:layout_marginTop="300dp"
            android:layout_toRightOf="@+id/reaccion_20cm"
            android:text="Reaccion\n(25 cm)" />

        <Button
            android:id="@+id/actualizar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/reaccion_25cm"
            android:layout_alignStart="@+id/reaccion_25cm"
            android:layout_below="@+id/reaccion_25cm"
            android:text="Actualizar" />

        <Button
            android:id="@+id/actualizar2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/reaccion_25cm"
            android:layout_toStartOf="@+id/reaccion_25cm"
            android:text="Actualizar2" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/motorLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tituloMotores"
            android:layout_width="404dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/menuLayout"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="70dp"
            android:gravity="center_horizontal"
            android:text="Control de Motores"
            android:textSize="35sp" />

        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="295dp"
            android:layout_above="@+id/seleccionMotor"
            android:layout_below="@+id/tituloMotores"
            android:layout_marginBottom="50dp"
            android:layout_marginTop="50dp">

            <Button
                android:id="@+id/motorR"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_above="@+id/power"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignTop="@+id/motorL"
                android:text="R" />

            <Button
                android:id="@+id/motorL"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_above="@+id/power"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:text="L" />

            <SeekBar
                android:id="@+id/power"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:id="@+id/datosMotor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/motorR"
                android:layout_toEndOf="@+id/motorR"
                android:layout_toRightOf="@+id/motorR"
                android:text="Recibiendo Datos..." />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/seleccionMotor"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentStart="true">

            <Button
                android:id="@+id/motor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:text="1" />

            <Button
                android:id="@+id/motor2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:text="2" />

            <Button
                android:id="@+id/motor3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:text="3" />
        </RelativeLayout>

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/menuLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:gravity="center_horizontal"
        android:measureWithLargestChild="false"
        android:orientation="horizontal">

        <Button
            android:id="@+id/sLink"
            style="@style/Widget.AppCompat.Button.Small"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/sLink"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_toLeftOf="@+id/button12"
            android:layout_toStartOf="@+id/button12"
            android:elevation="0dp"
            android:text="Link" />

        <Button
            android:id="@+id/sMotores"
            style="@style/Widget.AppCompat.Button.Small"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:text="  Motor  " />

        <Button
            android:id="@+id/sSensores"
            style="@style/Widget.AppCompat.Button.Small"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/sensor2"
            android:layout_alignParentTop="true"
            android:layout_alignRight="@+id/sensor2"
            android:layout_toEndOf="@+id/button12"
            android:layout_toRightOf="@+id/button12"
            android:text="Sensor" />

        <Button
            android:id="@+id/Ejecucion"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:text="Execute" />


    </LinearLayout>

    <RelativeLayout

        android:id="@+id/linkLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tituloLink"
            android:layout_width="404dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/menuLayout"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="70dp"
            android:background="@color/accent_material_light"
            android:gravity="center_horizontal"
            android:text="Conectar"
            android:textColor="@color/background_floating_material_light"
            android:textSize="35sp" />


        <ToggleButton
            android:id="@+id/bluetooth"
            android:layout_width="130dp"
            android:layout_height="120dp"
            android:layout_alignLeft="@+id/colorRobot"
            android:layout_alignStart="@+id/colorRobot"
            android:layout_below="@+id/etiquetacolor"
            android:layout_marginTop="19dp"
            android:background="@color/accent_material_light"
            android:checked="false"
            android:text="BT"
            android:textAllCaps="true"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@android:color/background_light"
            android:textOff="Conectar"
            android:textOn="Desconectar"
            android:textStyle="bold" />

        <ToggleButton
            android:id="@+id/habilitar"
            android:layout_width="130dp"
            android:layout_height="120dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/bluetooth"
            android:layout_marginEnd="21dp"
            android:layout_marginRight="21dp"
            android:background="@color/accent_material_light"
            android:checked="false"
            android:text="Habilitar/Deshabilitar"
            android:textColor="@android:color/background_light"
            android:textOff="Deshabilitar sensores"
            android:textOn="Habilitar sensores" />

        <TextView
            android:id="@+id/etiquetacolor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/tituloLink"
            android:background="@color/accent_material_dark"
            android:text="color"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold" />

        <ToggleButton
            android:id="@+id/iniciarcolor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/habilitar"
            android:layout_alignLeft="@+id/habilitar"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@+id/habilitar"
            android:layout_alignStart="@+id/habilitar"
            android:layout_marginBottom="28dp"
            android:background="@color/accent_material_dark"
            android:text="Modo Automatico Activado/Desactivado"
            android:textColor="?attr/colorAccent"
            android:textOff="Modo Automatico Desactivado"
            android:textOn="Modo Automatico Activado"
            android:textSize="12sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/buttonlogout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/iniciarcolor"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginBottom="28dp"
            android:background="@android:color/holo_red_dark"
            android:text="Salir"
            android:textColor="@color/primary_text_disabled_material_dark"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/texto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/bluetooth"
            android:layout_marginTop="12dp"
            android:background="@color/accent_material_light"
            android:text="Rastreo"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@color/bright_foreground_inverse_material_light"
            android:textStyle="bold" />

        <Button
            android:id="@+id/rastrear"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/datoObjetivox"
            android:layout_alignLeft="@+id/colorObjetivo"
            android:layout_alignRight="@+id/datoObjetivox"
            android:layout_alignStart="@+id/colorObjetivo"
            android:layout_alignTop="@+id/iniciarcolor"
            android:background="@color/accent_material_light"
            android:text="Rastreo"
            android:textColor="@android:color/background_light" />

        <Button
            android:id="@+id/detener"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/datoObjetivox"
            android:layout_alignLeft="@+id/colorObjetivo"
            android:layout_alignRight="@+id/datoObjetivox"
            android:layout_alignStart="@+id/colorObjetivo"
            android:layout_alignTop="@+id/iniciarcolor"
            android:background="@android:color/holo_red_dark"
            android:text="Detener"
            android:textColor="@android:color/background_light"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/texto"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="12dp"
            android:text="Datos en Firebase"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/colorRobot"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView6"
            android:layout_marginLeft="22dp"
            android:layout_marginStart="22dp"
            android:text="color" />

        <TextView
            android:id="@+id/colorObjetivo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/colorRobot"
            android:layout_alignStart="@+id/colorRobot"
            android:layout_below="@+id/colorRobot"
            android:text="Negro" />

        <TextView
            android:id="@+id/datoColorx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/colorObjetivo"
            android:layout_alignEnd="@+id/bluetooth"
            android:layout_alignRight="@+id/bluetooth"
            android:text="0" />

        <TextView
            android:id="@+id/datoObjetivox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/datoColorx"
            android:layout_alignStart="@+id/datoColorx"
            android:layout_below="@+id/datoColorx"
            android:text="0" />

        <TextView
            android:id="@+id/datoColory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/datoObjetivoy"
            android:layout_alignStart="@+id/datoObjetivoy"
            android:layout_below="@+id/textView6"
            android:text="0" />

        <TextView
            android:id="@+id/datoObjetivoy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/datoColory"
            android:layout_toLeftOf="@+id/buttonlogout"
            android:layout_toStartOf="@+id/buttonlogout"
            android:text="0" />

        <TextView
            android:id="@+id/azulx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/datoColorx"
            android:layout_alignStart="@+id/datoColorx"
            android:layout_alignTop="@+id/datoColorx"
            android:text="0" />

        <TextView
            android:id="@+id/azuly"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/datoColory"
            android:layout_alignBottom="@+id/datoColory"
            android:layout_alignLeft="@+id/datoColory"
            android:layout_alignStart="@+id/datoColory"
            android:text="0" />

        <TextView
            android:id="@+id/verdex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/datoObjetivox"
